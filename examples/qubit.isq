import std;

defgate AdjS = [1, 0;
                0, 0-1.0j];

defgate R1t = [1, 0;
               0, -0.5+0.8660254037844386j];


procedure CreateQubit(qbit q) {
	H<q>;
	R1t<q>;
	H<q>;
	S<q>;
}

procedure main()
{
	int r0 = 0;
	int r1 = 0;

	for i in 0:10000 {
		qbit Q;
		CreateQubit(Q);
		if (M(Q)) {
			r0 = r0 + 1;
		} else {
			r1 = r1 + 1;
		}
	}
	print r0;
	print r1;
}
